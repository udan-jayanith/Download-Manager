{
	"manifest_version": 3,
	"name": "Download Manager",
	"version": "0.0.0",
	"description": "Download manager",
	"icons": {
		"128": "assets/favicon.png"
	},
	"action": {
		"default_popup": "popup.html"
	},
	"permissions": ["webRequest", "activeTab", "storage"],
	"host_permissions": ["<all_urls>"],
	"background": {
		"service_worker": "service-worker.js"
	}
}
